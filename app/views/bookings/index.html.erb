<div class="container-fluid">
  <h1>List all Bookings</h1>
    <% @bookings.each do |booking| %>
      <%= link_to "Edit this booking", edit_car_booking_path(booking) %>
      <div class="p-3 mb-2">
        <h6>
          <%= booking.car.model%> -
          <%= booking.start_date%> -
          <%= booking.end_date%> -
          <%= booking.status%>
        </h6>
      </div>
    <% end %>
</div>
